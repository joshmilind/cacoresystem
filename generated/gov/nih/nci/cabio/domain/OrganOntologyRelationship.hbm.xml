<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.cabio.domain">
  <class name="OrganOntologyRelationship" table="ORGANONTOLOGYRELATIONSHIP" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="CHILD_ID_2">
      <generator class="assigned" />
    </id>
    <property name="bigid" type="java.lang.String" column="BIG_ID" />
    <property name="type" type="java.lang.String" column="RELATIONSHIP" />
    <many-to-one name="childOrganOntology" class="gov.nih.nci.cabio.domain.OrganOntology" column="CHILD_ID" lazy="proxy" />
    <many-to-one name="parentOrganOntology" class="gov.nih.nci.cabio.domain.OrganOntology" column="PARENT_ID" lazy="proxy" />
  </class>
</hibernate-mapping>

