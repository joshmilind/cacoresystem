<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.cadsr.domain">
  <class name="ClassificationSchemeItemRelationship" table="CABIO31_CSI_RECS_VIEW" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.String" column="CSI_REC_IDSEQ">
      <generator class="assigned" />
    </id>
    <property name="createdBy" type="java.lang.String" column="CREATED_BY" />
    <property name="dateCreated" type="java.util.Date" column="DATE_CREATED" />
    <property name="dateModified" type="java.util.Date" column="DATE_MODIFIED" />
    <property name="modifiedBy" type="java.lang.String" column="MODIFIED_BY" />
    <property name="name" type="java.lang.String" column="RL_NAME" />
    <many-to-one name="parentClassificationSchemeItem" class="gov.nih.nci.cadsr.domain.ClassificationSchemeItem" column="P_CSI_IDSEQ" lazy="proxy" />
    <many-to-one name="childClassificationSchemeItem" class="gov.nih.nci.cadsr.domain.ClassificationSchemeItem" column="C_CSI_IDSEQ" lazy="proxy" />
  </class>
</hibernate-mapping>

