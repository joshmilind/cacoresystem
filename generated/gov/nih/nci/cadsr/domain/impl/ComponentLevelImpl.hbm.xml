<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.cadsr.domain.impl">
  <class name="ComponentLevelImpl" table="CABIO31_CL_VIEW" lazy="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.String" column="CL_IDSEQ">
      <generator class="assigned" />
    </id>
    <property name="concatenationString" type="java.lang.String" column="CONCATENATION_STRING" />
    <property name="level" type="java.lang.Integer" column="COMPONENT_LEVEL" />
  </class>
</hibernate-mapping>

