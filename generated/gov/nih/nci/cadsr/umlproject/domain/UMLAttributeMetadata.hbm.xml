<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.cadsr.umlproject.domain">
  <class name="UMLAttributeMetadata" table="UP_ATTRIBUTE_METADATA_MVW" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.String" column="AM_IDSEQ">
      <generator class="assigned" />
    </id>
    <property name="description" type="java.lang.String" column="DESCRIPTION" />
    <property name="fullyQualifiedName" type="java.lang.String" column="FULLY_QUALIFIED_NAME" />
    <property name="name" type="java.lang.String" column="NAME" />
    <many-to-one name="UMLClassMetadata" class="gov.nih.nci.cadsr.umlproject.domain.UMLClassMetadata" column="CM_IDSEQ" lazy="proxy" />
    <many-to-one name="dataElement" class="gov.nih.nci.cadsr.domain.DataElement" column="DE_IDSEQ" lazy="proxy" />
    <many-to-one name="project" class="gov.nih.nci.cadsr.umlproject.domain.Project" column="CP_IDSEQ" lazy="proxy" />
    <many-to-one name="attributeTypeMetadata" class="gov.nih.nci.cadsr.umlproject.domain.AttributeTypeMetadata" column="AT_IDSEQ" lazy="proxy" />
    <set name="semanticMetadataCollection" lazy="true">
      <cache usage="read-write" />
      <key column="AM_IDSEQ" />
      <one-to-many class="gov.nih.nci.cadsr.umlproject.domain.SemanticMetadata" />
    </set>
  </class>
</hibernate-mapping>

