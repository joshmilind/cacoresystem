<transformation>
	<model-access 
		className="gov.nih.nci.codegen.core.access.UML13ModelAccess"
		xmiURI="file:@LOGICAL_MODEL@"
		modelName="EA Model"/>
	
	<filter-chain>
     <filter 
     	name="model-filter" 
     	className="gov.nih.nci.codegen.core.filter.UML13ModelFilter">
			<param name="modelName" value="EA Model"/>
     	</filter>
	</filter-chain>

	<transformer-chain>
	 <transformer name="dao"
	 	className="gov.nih.nci.codegen.core.transformer.UML13DAOTransformer">
	 	<param name="basePackage" value="Logical View.Logical Model"/>
	 	<param name="database" value="@DATABASE@"/>
	 	<param name="externalServer" value="@EXTERNALSERVER@"/>
	 	<param name="evsDtsrpcServer" value="@EVSDTSRPCSERVER@"/>
	 	<param name="evsDtsrpcPort" value="@EVSDTSRPCPORT@"/>
	 	<param name="evsMetaphraseServer" value="@EVSMETAPHRASESERVER@"/>
	 	<param name="evsMetaphraseDatabase" value="@EVSMETAPHRASEDATABASE@"/>
	 	<param name="evsMetaphraseUsername" value="@EVSMETAPHRASEUSERNAME@"/>
	 	<param name="evsMetaphrasePassword" value="@EVSMETAPHRASEPASSWORD@"/>
	 	
	  <filter name="classifier" className="gov.nih.nci.codegen.core.filter.UML13ClassifierFilter">
	   <package>
	    <include>@INCLUDE_PACKAGE@</include>
	   </package>
	  </filter>
	 </transformer>
	</transformer-chain>

	<artifact-handler 
		className="gov.nih.nci.codegen.core.handler.SimpleFileHandler">
		<param name="outputDir" value="@OUTPUTDIR@"/>
		<param name="fileName" value="DAOConfig.xml"/>
	</artifact-handler>

</transformation>